{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@adbox/zeepay/(.*)": "<rootDir>/../libs/zeepay/src/$1",
    "@adbox/zeepay": "<rootDir>/../libs/zeepay/src"
  }
}