{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@adbox/zeepay/(.*)": "<rootDir>/../libs/zeepay/src/$1",
    "@adbox/zeepay": "<rootDir>/../libs/zeepay/src",
    "@adbox/utils/(.*)": "<rootDir>/../libs/utils/src/$1",
    "@adbox/utils": "<rootDir>/../libs/utils/src",
    "@adbox/junipay/(.*)": "<rootDir>/../libs/junipay/src/$1",
    "@adbox/junipay": "<rootDir>/../libs/junipay/src",
    "@adbox/arkesel/(.*)": "<rootDir>/../libs/arkesel/src/$1",
    "@adbox/arkesel": "<rootDir>/../libs/arkesel/src"
  }
}