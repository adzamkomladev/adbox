  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@adbox/zeepay(|/.*)$": "<rootDir>/libs/zeepay/src/$1",
      "^@adbox/utils(|/.*)$": "<rootDir>/libs/utils/src/$1",
      "^@adbox/junipay(|/.*)$": "<rootDir>/libs/junipay/src/$1",
      "^@adbox/arkesel(|/.*)$": "<rootDir>/libs/arkesel/src/$1",
